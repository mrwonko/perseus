// The function must be impure to be allowed to call the impure builtin print()
impure function print_if_big( x : i32 )
    if x >= 10
        print( x )

impure function main()
{
    print_if_big( 0 ); // should not result in a print
    print_if_big( 10 );
    print_if_big( 42 );
    print_if_big( 1337 )
}